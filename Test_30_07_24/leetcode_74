  class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
        int n = matrix.size();
        int m = matrix[0].size();
        for(int i=0;i<n;i++){
            if(target >= matrix[i][0] and target<= matrix[i][m-1]){
                int l = 0;
                int r = m-1;
                while(l<=r){
                    int mid = (l+r)/2;
                    if(matrix[i][mid] > target){
                        r = mid - 1;
                    }else if(matrix[i][mid] < target){
                        l = mid + 1;
                    }else{
                        return true;
                    }
                }
                return false;
            }
        }
        return false;
    }
};
